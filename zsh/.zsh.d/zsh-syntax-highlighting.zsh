#### zsh-syntax-highlighting ####
ZSH_HIGHLIGHT_HIGHLIGHTERS=(main)
ZSH_HIGHLIGHT_STYLES[command]='fg=#a8ff60,bold'
ZSH_HIGHLIGHT_STYLES[unknown-token]='fg=#ff5f87,bold'
ZSH_HIGHLIGHT_STYLES[path]='fg=#5fd7ff'
ZSH_HIGHLIGHT_STYLES[alias]='fg=#87d7ff,italic'
ZSH_HIGHLIGHT_STYLES[reserved-word]='fg=#ffd75f,bold'
ZSH_HIGHLIGHT_STYLES[globbing]='fg=#ffaf00'
ZSH_HIGHLIGHT_STYLES[single-hyphen-option]='fg=#ffffaf'
ZSH_HIGHLIGHT_STYLES[double-hyphen-option]='fg=#ffffaf'
